{
	"presets": [
		["latest", {
			"es2015": {
				"modules": false
			}
		}],
		"react"
	],
	"plugins": [
		"transform-decorators-legacy",
		"transform-class-properties",
		"transform-object-rest-spread",
		["transform-regenerator", {
			"async": false
		}],
		["transform-runtime", {
			"helpers": false,
			"polyfill": false,
			"regenerator": true
		}]
	],
	"env": {
		"dev": {
			"plugins": [
				["react-transform", {
					"transforms": [{
						"transform": "react-transform-hmr",
						"imports": ["react"],
						"locals": ["module"]
					}]
				}]
			]
		}
	}
}
